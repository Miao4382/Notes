\part{824. Goat Latin}
\label{sec:org250f3b9}
\chapter{Problem Statement}
\label{sec:org2551a19}
\href{https://leetcode.com/problems/goat-latin/}{Link}
\chapter{Analysis}
\label{sec:orgd50386d}
\section{Modify the word directly}
\label{sec:org6671694}
The problem has already given the instructions on how to modify the word.
\chapter{Solution}
\label{sec:orgfcf1b4f}
\section{Python}
\label{sec:org45d7a5f}
\subsection{modify the word directly}
\label{sec:org6fe37e4}
\begin{minted}[breaklines=true,breakanywhere=true]{python}
class Solution:
    def toGoatLatin(self, S: str) -> str:
	words = S.split()

	vowel = ('a', 'e', 'i', 'o', 'u')

	for i in range(len(words)):

	    if words[i][0].lower() in vowel:
		words[i] += "ma"
	    else:
		words[i] = words[i][1:] + words[i][0] + "ma"

	    for j in range(i + 1):
		words[i] += 'a'

	    words[i] += ' '

	words[-1] = words[-1][:-1]

	return ''.join(words)
\end{minted}
