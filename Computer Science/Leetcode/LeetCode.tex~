\chapter{771. Jewels and Stones}
\label{sec:org14f096f}
\section{Problem Statement}
\label{sec:org52a081a}
\href{https://leetcode.com/problems/jewels-and-stones/}{Link}
\section{Analysis}
\label{sec:org1713d86}
\section{Solution}
\label{sec:orgd223932}
\subsection{C++}
\label{sec:org48e4a53}
\subsubsection{\(N^2\) Time (96.35\%) Space (79.64\%)}
\label{sec:org0b16750}
\begin{minted}[linenos,firstnumber=1,breaklines=true,breakanywhere=true]{c++}
class Solution {
public:
  int numJewelsInStones(string J, string S) {
    int numJewl = 0;
    for (auto s : S)
      if (isJewels(s, J))
	numJewl++;
    return numJewl;
  }

  bool isJewels(char s, string J) {
    for (auto j : J)
      if (s == j)
	return true;

    return false;
  }
};
\end{minted}
\part{Trees}
\label{sec:orga1bcc36}
\chapter{104. Maximum Depth of Binary Tree}
\label{sec:org9b3ea00}
\section{Problem Statement}
\label{sec:org6b21ba9}
\href{https://leetcode.com/problems/maximum-depth-of-binary-tree/}{Link}
\section{Analysis}
\label{sec:org5222fe5}
\subsection{Recursion}
\label{sec:orgc80ed5d}
A node's maximum depth, is the larger maximum depth of its left and right subtree plus one. Base case: if a node is nullptr, maximum depth is zero.
\section{Solution}
\label{sec:orgd8f2b7a}
\subsection{C++}
\label{sec:org772a2ac}
\subsubsection{Recursion. Time (88.44\%) Space (91.28\%)}
\label{sec:org37b6e71}
\begin{minted}[linenos,firstnumber=1,breaklines=true,breakanywhere=true]{c++}
/**
 * Definition for a binary tree node.
 * struct TreeNode {
 *     int val;
 *     TreeNode *left;
 *     TreeNode *right;
 *     TreeNode(int x) : val(x), left(NULL), right(NULL) {}
 * };
 */
class Solution {
public:
  int maxDepth(TreeNode* root) {
    // base case 
    if (root == nullptr)
      return 0;

    int left_depth = maxDepth(root->left);
    int right_depth = maxDepth(root->right);

    return (left_depth >= right_depth ? left_depth + 1 : right_depth + 1);
  }
};
\end{minted}
