\chapter{771. Jewels and Stones}
\label{sec:org4268a14}
\section{Problem Statement}
\label{sec:org3064a57}
\href{https://leetcode.com/problems/jewels-and-stones/}{Link}
\section{Analysis}
\label{sec:org5f55b82}
\section{Solution}
\label{sec:org709667e}
\subsection{C++}
\label{sec:org5c88853}
\subsubsection{\(N^2\) Time (96.35\%) Space (79.64\%)}
\label{sec:org4e23a9f}
\begin{minted}[linenos,firstnumber=1,breaklines=true,breakanywhere=true]{c++}
class Solution {
public:
  int numJewelsInStones(string J, string S) {
    int numJewl = 0;
    for (auto s : S)
      if (isJewels(s, J))
	numJewl++;
    return numJewl;
  }

  bool isJewels(char s, string J) {
    for (auto j : J)
      if (s == j)
	return true;

    return false;
  }
};
\end{minted}
\part{Trees}
\label{sec:org41d767d}
\chapter{104. Maximum Depth of Binary Tree}
\label{sec:org0ac3e61}
\section{Problem Statement}
\label{sec:org844c8b8}
\href{https://leetcode.com/problems/maximum-depth-of-binary-tree/}{Link}
\section{Analysis}
\label{sec:org7f8eb99}
\subsection{Recursion}
\label{sec:orgf0eb9d6}
A node's maximum depth, is the larger maximum depth of its left and right subtree plus one. Base case: if a node is nullptr, maximum depth is zero.
\section{Solution}
\label{sec:org6bb7c85}
\subsection{C++}
\label{sec:org42d6251}
\subsubsection{Recursion. Time (88.44\%) Space (91.28\%)}
\label{sec:org5577fbb}
\begin{minted}[linenos,firstnumber=1,breaklines=true,breakanywhere=true]{c++}
/**
 * Definition for a binary tree node.
 * struct TreeNode {
 *     int val;
 *     TreeNode *left;
 *     TreeNode *right;
 *     TreeNode(int x) : val(x), left(NULL), right(NULL) {}
 * };
 */
class Solution {
public:
  int maxDepth(TreeNode* root) {
    // base case 
    if (root == nullptr)
      return 0;

    int left_depth = maxDepth(root->left);
    int right_depth = maxDepth(root->right);

    return (left_depth >= right_depth ? left_depth + 1 : right_depth + 1);
  }
};
\end{minted}
\section{todos [/]}
\label{sec:org7ec6c49}
\begin{itemize}
\item[{$\square$}] read about the discussion page for more methods and ideas
\end{itemize}

\chapter{617. Merge Two Binary Trees}
\label{sec:org2aac848}
\section{Problem Statement}
\label{sec:org4f49f06}
\href{https://leetcode.com/problems/merge-two-binary-trees/}{Link}
\section{Analysis}
\label{sec:org4c23e52}
\subsection{Approach \#1 Recursive Method}
\label{sec:org79128c6}
Use recursiion to solve this problem.
\subsection{Approach \#2 Iterative Method}
\label{sec:orgc0a89f0}

\section{Solution}
\label{sec:org7a0e162}
\subsection{C++}
\label{sec:org3827720}
\subsubsection{Recursion Time (97.09\%) Space(37.01\%)}
\label{sec:org8e029e0}
Recursion.
\begin{minted}[linenos,firstnumber=1,breaklines=true,breakanywhere=true]{c++}
/**
 * Definition for a binary tree node.
 * struct TreeNode {
 *     int val;
 *     TreeNode *left;
 *     TreeNode *right;
 *     TreeNode(int x) : val(x), left(NULL), right(NULL) {}
 * };
 */
class Solution {
public:
  TreeNode* mergeTrees(TreeNode* t1, TreeNode* t2) {
    if (t1 == nullptr)
      return t2;
    else if (t2 == nullptr)
      return t1;
    else {
      TreeNode* node = new TreeNode(t1->val + t2->val);
      node->left = mergeTrees(t1->left, t2->left);
      node->right = mergeTrees(t1->right, t2->right);
      return node;
    }
  }
};
\end{minted}
\subsubsection{Iterative}
\label{sec:org797ee5d}
