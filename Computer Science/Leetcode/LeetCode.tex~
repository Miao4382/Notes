\label{sec:orga3b3082}
\subsection{direct replace}
\label{sec:org1acc0b6}
\begin{minted}[breaklines=true,breakanywhere=true]{python}
class Solution:
    def defangIPaddr(self, address: str) -> str:
	return address.replace('.', '[.]')
\end{minted}
