\PYG{k}{class} \PYG{n+nc}{Solution}\PYG{p}{:}
    \PYG{k}{def} \PYG{n+nf}{countBinarySubstrings}\PYG{p}{(}\PYG{n+nb+bp}{self}\PYG{p}{,} \PYG{n}{s}\PYG{p}{:} \PYG{n+nb}{str}\PYG{p}{)} \PYG{o}{\PYGZhy{}\PYGZgt{}} \PYG{n+nb}{int}\PYG{p}{:}
	\PYG{n}{num\PYGZus{}first} \PYG{o}{=} \PYG{p}{[]}
	\PYG{n}{num\PYGZus{}second} \PYG{o}{=} \PYG{p}{[]}
	\PYG{n}{result} \PYG{o}{=} \PYG{l+m+mi}{0}

	\PYG{n}{num\PYGZus{}first}\PYG{o}{.}\PYG{n}{append}\PYG{p}{(}\PYG{n}{s}\PYG{p}{[}\PYG{l+m+mi}{0}\PYG{p}{])}

	\PYG{k}{for} \PYG{n}{i} \PYG{o+ow}{in} \PYG{n+nb}{range}\PYG{p}{(}\PYG{l+m+mi}{1}\PYG{p}{,} \PYG{n+nb}{len}\PYG{p}{(}\PYG{n}{s}\PYG{p}{)):}
	    \PYG{k}{if} \PYG{n+nb}{len}\PYG{p}{(}\PYG{n}{num\PYGZus{}second}\PYG{p}{)} \PYG{o}{==} \PYG{l+m+mi}{0} \PYG{o+ow}{and} \PYG{n}{s}\PYG{p}{[}\PYG{n}{i}\PYG{p}{]} \PYG{o}{==} \PYG{n}{num\PYGZus{}first}\PYG{p}{[}\PYG{l+m+mi}{0}\PYG{p}{]:}
		\PYG{n}{num\PYGZus{}first}\PYG{o}{.}\PYG{n}{append}\PYG{p}{(}\PYG{n}{s}\PYG{p}{[}\PYG{n}{i}\PYG{p}{])}

	    \PYG{k}{elif} \PYG{n}{s}\PYG{p}{[}\PYG{n}{i}\PYG{p}{]} \PYG{o}{!=} \PYG{n}{num\PYGZus{}first}\PYG{p}{[}\PYG{l+m+mi}{0}\PYG{p}{]:}
		\PYG{n}{num\PYGZus{}second}\PYG{o}{.}\PYG{n}{append}\PYG{p}{(}\PYG{n}{s}\PYG{p}{[}\PYG{n}{i}\PYG{p}{])}

