		\PYG{n}{result} \PYG{o}{+=} \PYG{n+nb}{min}\PYG{p}{(}\PYG{n+nb}{len}\PYG{p}{(}\PYG{n}{num\PYGZus{}first}\PYG{p}{),} \PYG{n+nb}{len}\PYG{p}{(}\PYG{n}{num\PYGZus{}second}\PYG{p}{))}
		\PYG{n}{num\PYGZus{}first} \PYG{o}{=} \PYG{n}{num\PYGZus{}second}\PYG{p}{[:]}
		\PYG{n}{num\PYGZus{}second}\PYG{o}{.}\PYG{n}{clear}\PYG{p}{()}
